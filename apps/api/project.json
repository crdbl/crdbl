{
  "name": "@crdbl/api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "dependsOn": ["^build"],
      "options": {
        "script": "build:ts"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --check apps/api"
      }
    },
    "serve": {
      "executor": "nx:run-script",
      "options": {
        "script": "start"
      }
    }
  }
}
